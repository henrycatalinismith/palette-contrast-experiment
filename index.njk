---
title: contrast
description: Contrast ratio accessibility analysis
---
<!doctype html>
<html
  lang="en"
  dir="ltr">
  <head>
    <meta charset="utf-8" />

    <link
      rel="canonical"
      href="https://hen.cat/contrast"
    />

    <meta
      name="viewport"
      content="width=device-width, initial-scale=1"
    />

    <link
      rel="stylesheet"
      href="{{ collections["11tysass"]["style.scss"].stats.entry }}"
    />

    <title>
      {{ title }}
    </title>
  </head>
  <body>
    <main>
      <header>
        <h1>
          {{ title }}
        </h1>
        <p>
          {{ description }}
        </p>
      </header>
      <dl>
        {% for palette in collections.palettes %}
          <dt>
            <a href="{{ url }}/{{ palette.data.slug }}">
              {{ palette.data.name }}
            </a>
          </dt>
          <dd>
            {{ palette.data.description }}
          </dd>
        {% endfor %}
      </dl>

    </main>
  </body>
</html>

